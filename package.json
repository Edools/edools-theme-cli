{
  "name": "edools-theme-cli",
  "description": "A starter kit to make Edools Themes",
  "version": "3.1.0",
  "author": "Mayke Freitas",
  "bin": {
    "edools-theme": "./bin/index.js",
    "edt": "./bin/index.js",
    "edools-theme-cli": "./bin/run"
  },
  "bugs": {
    "url": "https://github.com/Edools/edools-theme-cli/issues"
  },
  "dependencies": {
    "@oclif/command": "^1.5.11",
    "@oclif/config": "^1.12.8",
    "@oclif/plugin-help": "^2.1.6",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.26.3",
    "chalk": "^2.4.2",
    "commander": "^2.19.0",
    "csscomb": "^4.2.0",
    "del": "^4.0.0",
    "glob-all": "^3.1.0",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-changed": "^3.2.0",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-if": "^2.0.2",
    "gulp-load-plugins": "^1.5.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-strip-css-comments": "^2.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-util": "^3.0.8",
    "inquirer": "^6.2.2",
    "lodash": "^4.17.11",
    "merge-stream": "^1.0.1",
    "pretty-hrtime": "^1.0.3",
    "request": "^2.88.0",
    "run-sequence": "^2.2.1",
    "simple-git": "^1.107.0",
    "through2": "^3.0.1",
    "wiredep": "^4.0.0",
    "wrench": "^1.5.9"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.3.4",
    "@oclif/dev-cli": "^1.21.2",
    "@oclif/test": "^1.2.4",
    "chai": "^4.2.0",
    "eslint": "^5.14.1",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^8.0.2",
    "jshint": "^2.10.2",
    "jshint-stylish": "^2.2.1",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "watch": "^1.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/Edools/edools-theme-cli",
  "keywords": [
    "edools",
    "theme"
  ],
  "license": "ISC",
  "main": "index",
  "oclif": {
    "commands": "./src/commands",
    "bin": "edools-theme-cli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Edools/edools-theme-cli"
  },
  "scripts": {
    "dev": "watch 'npm run lint' .",
    "lint": "jshint --reporter=node_modules/jshint-stylish bin || true",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
